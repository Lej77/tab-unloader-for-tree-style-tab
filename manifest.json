{
  "manifest_version": 2,
  "name": "Tab Unloader for Tree Style Tab",
  "version": "3.1",
  "default_locale": "en",
  "applications": {
    "gecko": {
      "strict_min_version": "58.0"
    }
  },
  "options_ui": {
    "page": "options/options.html"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts": [
      "common.js",
      "background.js"
    ]
  }
}